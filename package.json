{
  "name": "bfx-hf-ext-plugin-bitfinex",
  "version": "1.0.7",
  "description": "Bitfinex exchange plugin for the Honey Framework",
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "Apache-2.0",
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-ext-plugin-bitfinex.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-ext-plugin-bitfinex/issues"
  },
  "scripts": {
    "lint": "eslint examples/ test/ lib/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 90 --branches 60 --functions 90 --statements 90 --reporter=lcov --reporter=html mocha -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-api-node-core": "^1.1.8",
    "bfx-api-node-models": "^1.1.9",
    "bfx-api-node-plugin-managed-candles": "^1.0.0",
    "bfx-api-node-plugin-managed-ob": "^1.0.0",
    "bfx-api-node-plugin-wd": "^1.0.0",
    "bfx-api-node-rest": "^3.0.4",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-util": "^1.0.5",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "p-iteration": "^1.1.8",
    "promise-throttle": "^1.0.0"
  },
  "devDependencies": {
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.2.0"
  }
}
